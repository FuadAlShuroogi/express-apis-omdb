<br>

<ul>

<% if(results === undefined){ %>
    <img src="/images/notfound.jpeg" alt="">
    <% } else{ %>  
    <h2>Click on the below to get the movies details !</h2>
    <% results.forEach(m=>{ %>

        <h3>    
        <li>
        <a href="/movies/<%=m.imdbID%>">
        <%= m.Title %>
        </a> 
        </li>
        </h3>
        <%}) %>
    <% } %>

</ul>
